{
  "private": true,
  "scripts": {
    "build": "npm run copy-static && npm run build-html && npm run build-webpack",
    "copy-static": "cpx \"static/**/*\" dist --clean",
    "build-html": "eleventy --config=./build/eleventy.js",
    "build-webpack": " webpack --mode production --config ./build/webpack.config.js",
    "dev": "concurrently \"npm run watch-static\" \"npm run watch-html\" \"npm run dev-webpack\"",
    "watch-static": "cpx \"static/**/*\" dist --watch",
    "watch-html": "eleventy --watch --config=./build/eleventy.js",
    "dev-webpack": "webpack-dev-server --mode development --config ./build/webpack.config.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "copy-and-watch": "^0.1.4",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "cpx": "^1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "browserslist": [
    "ie >= 11",
    "safari >= 9",
    "ios >= 9",
    "Android >= 5"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
